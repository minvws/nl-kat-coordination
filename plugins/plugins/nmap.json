{
  "plugin_id": "nmap",
  "name": "Nmap TCP",
  "description": "Defaults to top 250 TCP ports. Includes service detection.",
  "scan_level": 2,
  "oci_image": "instrumentisto/nmap",
  "oci_arguments": [
    "--open",
    "-T4",
    "-Pn",
    "-r",
    "-v10",
    "-sV",
    "-sS",
    "-oX",
    "-",
    "${IP_ADDRESS}"
  ]
}
