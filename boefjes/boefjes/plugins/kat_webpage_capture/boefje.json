{
  "id": "webpage-capture",
  "name": "Webpage Capture",
  "description": "Takes a screenshot of a webpage. Image can be found on the Tasks page in the downloadable 'meta and raw data' file.",
  "consumes": [
    "HostnameHTTPURL",
    "IPAddressHTTPURL"
  ],
  "produces": [
    "image/png",
    "application/zip+json",
    "application/har+json",
    "application/json",
    "application/localstorage+json"
  ],
  "scan_level": 2,
  "oci_image": "ghcr.io/minvws/openkat/webpage-capture:latest",
  "oci_arguments": []
}
