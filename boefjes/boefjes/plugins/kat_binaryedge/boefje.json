{
    "id": "binaryedge",
    "name": "BinaryEdge",
    "description": "Use BinaryEdge to find open ports with vulnerabilities that are found on that port",
    "consumes": [
        "IPAddressV4",
        "IPAddressV6"
    ],
    "produces": [
        "KATFindingType",
        "SoftwareInstance",
        "Service",
        "IPPort",
        "Finding",
        "Software",
        "IPService",
        "CVEFindingType"
    ],
    "environment_keys": ["BINARYEDGE_API"],
    "scan_level": 2
}
