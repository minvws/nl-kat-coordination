{
  "id": "nmap-udp",
  "name": "Nmap UDP",
  "description": "Defaults to top 250 UDP ports. Includes service detection.",
  "consumes": [
    "IPAddressV4",
    "IPAddressV6"
  ],
  "scan_level": 2,
  "oci_image": "ghcr.io/minvws/openkat/nmap:latest",
  "oci_arguments": [
    "--open",
    "-T4",
    "-Pn",
    "-r",
    "-v10",
    "-sV",
    "-sU"
  ]
}
