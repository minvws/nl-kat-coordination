{
    "id": "external_db",
    "name": "External Database",
    "description": "Fetch hostnames and IP addresses/netblocks from an external database with API. See `description.md` for more information.",
    "consumes": [
        "Network"
    ],
    "produces": [
        "Hostname",
        "IPAddressV4",
        "IPV4NetBlock",
        "IPAddressV6",
        "IPV6NetBlock"
    ],
    "environment_keys": [
        "DB_URL",
        "DB_ACCESS_TOKEN",
        "DB_ORGANIZATION_IDENTIFIER",
        "DB_ENDPOINT_FORMAT",
        "REQUESTS_CA_BUNDLE"
    ],
    "scan_level": 0
}
