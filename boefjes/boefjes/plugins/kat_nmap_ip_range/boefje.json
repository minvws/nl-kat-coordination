{
    "id": "nmap-ip-range",
    "name": "Nmap IP range",
    "description": "Scan an IP range and store found IPs.",
    "consumes": [
        "IPV6NetBlock",
        "IPV4NetBlock"
    ],
    "produces": [
        "IPAddressV6",
        "Service",
        "IPPort",
        "IPAddressV4",
        "IPService"
    ],
    "environment_keys": [
        "TOP_PORTS_TCP",
        "TOP_PORTS_UDP",
        "MIN_VLSM_IPV4",
        "MIN_VLSM_IPV6"
    ],
    "scan_level": 2
}
