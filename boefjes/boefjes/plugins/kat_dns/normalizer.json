{
  "id": "kat_dns_normalize",
  "name": "DNS records",
  "description": "Parses DNS records. Can parse A, AAAA, CAA, CNAME, MX, NS, SOA, TXT, DKIM and DMARC data.",
  "consumes": [
    "boefje/dns-records"
  ],
  "produces": [
    "IPAddressV6",
    "DNSARecord",
    "DNSNSRecord",
    "DNSTXTRecord",
    "DNSSOARecord",
    "NXDOMAIN",
    "DNSCNAMERecord",
    "DNSMXRecord",
    "Hostname",
    "Network",
    "DNSAAAARecord",
    "IPAddressV4",
    "DNSZone"
  ]
}
