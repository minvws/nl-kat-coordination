{
  "version": 1,
  "disable_existing_loggers": 0,
  "formatters": {
    "default": {
      "format": "%(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "INFO",
      "stream": "ext://sys.stdout"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "console"
    ]
  }
}
