          },
          "allow_replace": {
            "title": "Allow Replace",
            "type": "boolean"
          },
          "allow_updates": {
            "title": "Allow Updates",
            "type": "boolean"
          },
          "allow_priority_updates": {
            "title": "Allow Priority Updates",
            "type": "boolean"
          },
          "pq": {
            "title": "Pq",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/PrioritizedItem"
            }
