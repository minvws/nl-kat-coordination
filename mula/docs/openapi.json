        "title": "Task",
        "required": [
          "id",
          "scheduler_id",
          "type",
          "p_item",
          "status"
        ],
        "type": "object",
        "properties": {
          "id": {
            "title": "Id",
            "type": "string",
            "format": "uuid"
          },
          "scheduler_id": {
            "title": "Scheduler Id",
