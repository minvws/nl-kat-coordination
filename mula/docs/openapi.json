        "title": "Queue",
        "required": [
          "id",
          "size",
          "maxsize",
          "item_type",
          "allow_replace",
          "allow_updates",
          "allow_priority_updates"
        ],
        "type": "object",
        "properties": {
          "id": {
            "title": "Id",
            "type": "string"
          },
          "size": {
            "title": "Size",
            "type": "integer"
          },
          "maxsize": {
            "title": "Maxsize",
            "type": "integer"
          },
          "item_type": {
            "title": "Item Type",
